{
  "name": "@iot-hub/acm-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "acm-e2e",
    "implicitDependencies": [
      "acm"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/acm-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "user-management:build",
          "user-management:serve"
        ]
      }
    }
  }
}
