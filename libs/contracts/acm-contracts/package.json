{
  "name": "@iot-hub/acm-contracts",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "options": {
          "outputPath": "dist/libs/contracts/acm-contracts",
          "main": "libs/contracts/acm-contracts/src/index.ts",
          "tsConfig": "libs/contracts/acm-contracts/tsconfig.lib.json"
        }
      }
    }
  }
}
